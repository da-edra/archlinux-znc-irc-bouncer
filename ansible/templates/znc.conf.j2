// WARNING
//
// Do NOT edit this file while ZNC is running!
// Use webadmin or *controlpanel instead.
//
// Altering this file by hand will forfeit all support.
//
// But if you feel risky, you might want to read help on /znc saveconfig and /znc rehash.
// Also check https://wiki.znc.in/Configuration

AnonIPLimit = 10
AuthOnlyViaModule = false
ConfigWriteDelay = 0
ConnectDelay = 5
HideVersion = true
LoadModule = webadmin
MaxBufferSize = 1000
ProtectWebSessions = true
SSLCertFile = /var/lib/znc/.znc/znc.pem
SSLDHParamFile = /var/lib/znc/.znc/znc.pem
SSLKeyFile = /var/lib/znc/.znc/znc.pem
ServerThrottle = 30
Skin = _default_
StatusPrefix = *
Version = 1.8.2

<Listener listener0>
	AllowIRC = true
	AllowWeb = true
	IPv4 = true
	IPv6 = true
	Port = 8000
	SSL = false
	URIPrefix = /
</Listener>

<Listener listener1>
	AllowIRC = true
	AllowWeb = false
	IPv4 = true
	IPv6 = true
	Port = 6697
	SSL = true
	URIPrefix = /
</Listener>

<User {{ znc_user }}>
	Admin = true
	Allow = *
	AltNick = {{ irc_nick }}_
	AppendTimestamp = false
	AuthOnlyViaModule = false
	AutoClearChanBuffer = false
	AutoClearQueryBuffer = true
	ChanBufferSize = 1000
	ClientEncoding = ^UTF-8
	DenyLoadMod = false
	DenySetBindHost = false
	Ident = {{ znc_user }}
	JoinTries = 10
	LoadModule = chansaver
	LoadModule = controlpanel
	LoadModule = cert
	MaxJoins = 0
	MaxNetworks = 1
	MaxQueryBuffers = 1000
	MultiClients = true
	Nick = {{ irc_nick }}
	NoTrafficTimeout = 180
	PrependTimestamp = true
	QueryBufferSize = 1000
	QuitMsg = %znc%
	RealName = ZNC - https://znc.in
	Skin = _default_
	TimestampFormat = [%H:%M:%S]
	Timezone = GMT-6

	<Network freenode>
		Encoding = ^UTF-8
		FloodBurst = 9
		FloodRate = 2.00
		IRCConnectEnabled = true
		Ident = {{ irc_nick }}
		JoinDelay = 0
		LoadModule = autocycle
		LoadModule = clientbuffer
		LoadModule = sasl
		LoadModule = simple_away
		LoadModule = cert
		LoadModule = autoattach
		LoadModule = keepnick
		LoadModule = kickrejoin
		LoadModule = log -sanitize
		RealName = {{ irc_nick }}
		Server = 10.99.99.90 +6697
		TrustAllCerts = false
		TrustPKI = true
		TrustedServerFingerprint = {{ freenode_trusted_server_fingerprint }}

		<Chan ##alerts>
		</Chan>

		<Chan ##linux>
		</Chan>

		<Chan ##news>
		</Chan>

		<Chan ##rust>
		</Chan>

		<Chan #archlinux>
		</Chan>

		<Chan #archlinux-arm>
		</Chan>

		<Chan #archlinux-aur>
			AutoClearChanBuffer = false
			Key = #archlinux-aurweb
		</Chan>

		<Chan #archlinux-aurweb>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-bugs>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-classroom>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-cn>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-conf>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-devops>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-es>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-multilib>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-mx>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-newbie>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-offtopic>
		</Chan>

		<Chan #archlinux-pacman>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-proaudio>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-projects>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-releng>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-reproducible>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-security>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-testing>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-wiki>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #archlinux-women>
			AutoClearChanBuffer = false
		</Chan>

		<Chan #bitcoin>
		</Chan>

		<Chan #cryptography-dev>
		</Chan>

		<Chan #emacs>
		</Chan>

		<Chan #ethereum>
		</Chan>

		<Chan #freenode>
