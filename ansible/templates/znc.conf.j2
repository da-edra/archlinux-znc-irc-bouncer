		<Chan #weechat>
		</Chan>

		<Chan #znc>
		</Chan>
	</Network>

	<Network oftc>
		Encoding = ^UTF-8
		FloodBurst = 9
		FloodRate = 2.00
		IRCConnectEnabled = true
		JoinDelay = 0
		LoadModule = cert
		LoadModule = clientbuffer
		LoadModule = sasl
		LoadModule = simple_away
		Server = 10.99.99.91 +6697
		TrustAllCerts = false
		TrustPKI = true

		<Chan tor>
			Disabled = true
		</Chan>
	</Network>

	<Pass password>
		Hash = {{ znc_password_hash }}
		Method = SHA256
		Salt = {{ znc_password_salt }}
	</Pass>
</User>
